public class CalculatorClass {
    private static integer VarSub;
    private static Integer VarDiv;
    public static integer MakeSub(integer VarNum1, integer VarNum2){
        VarSub = VarNum1 - VarNum2;
        return VarSub;
    }
    
    Public static double MakeDiv(Integer VarNumA, integer VarNumB){
        VarDiv = VarNumA / VarNumB;
        return VarDiv;
    }
    

}